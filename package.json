  "browserslist": [">0.2%", "not dead", "not ie <= 11", "not op_mini all"],
  "babel": {
    "presets": [
      [
        "@babel/env",
        {
          "targets": {
            "edge": "17",
            "firefox": "60",
            "chrome": "67",
            "safari": "11.1"
          },
          "useBuiltIns": "usage",
          "corejs": "3.6.5"
        }
      ],
      "@babel/preset-react"
    ]
