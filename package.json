{
  "name": "dribbble-atze",
  "version": "1.0.0",
  "description": "Test publishing design system for Dribbble class",
  "private": false,
  "main": "dist/index.js",
  "module": "dist/index.js",
  "keywords": [],
  "dependencies": {
